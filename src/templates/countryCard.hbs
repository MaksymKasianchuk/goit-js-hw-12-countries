    <div class="country-wrapper">
    <article class="card">
                <div class="card__content">
                    <h2 class="card__name">{{name}}</h2>
                    <p class="card__capital"> Capital:
                        {{capital}}
                    </p>
                    <p class="card__population"> Population:
                        {{population}}
                    </p>
                    <ul class="language-list list">
                         <h3 class="languages">Languages:</h3>
                        {{#each this.languages}}
                            <li class="language-list__item">{{name}}</li>
                        {{/each}}
                    </ul>
                </div>
                <div class="card__img-wrapper">
                    <img src="{{flag}}" alt="{{name}}" class="card__image">
                </div>
            </article>
    </div>
      